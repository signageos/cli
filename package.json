{
  "name": "@signageos/cli",
  "version": "1.1.5",
  "main": "./dist/index.js",
  "files": [
    ".env",
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "url": "https://github.com/signageos/cli",
    "type": "git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint --config node_modules/@signageos/codestyle/tslint.json {src,tests}/**/*.ts",
    "prebuild": "rm -rf dist/*",
    "prepare": "npm run prebuild && npm run build && chmod +x dist/index.js",
    "test": "env NODE_ENV=test mocha --config .mocharc.unit.json",
    "test-integration": "env NODE_ENV=test mocha --config .mocharc.integration.json",
    "watch": "tsc --watch",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib check-deps && npm run check-circular-deps",
    "check-circular-deps": "npx madge@5 --circular --extensions ts ./src/",
    "depcheck": "depcheck --config .depcheckrc.json"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "bin": {
    "sos": "./dist/index.js"
  },
  "publishConfig": {
    "registry": "https://npm.signageos.io",
    "access": "public"
  },
  "devDependencies": {
    "@signageos/codestyle": "0.0.21",
    "@types/chalk": "2.2.0",
    "@types/child-process-promise": "2.2.1",
    "@types/cli-progress": "3.7.0",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.2",
    "@types/cors": "2.8.6",
    "@types/debug": "4.1.4",
    "@types/express": "4.17.1",
    "@types/fs-extra": "8.0.0",
    "@types/globby": "9.1.0",
    "@types/internal-ip": "4.1.0",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.9",
    "@types/node-fetch": "2.5.0",
    "@types/prompts": "2.0.1",
    "@types/semver": "7.1.0",
    "@types/should": "13.0.0",
    "@types/sinon": "7.5.0",
    "@types/webpack": "4.41.7",
    "depcheck": "1.4.3",
    "mocha": "8.4.0",
    "rewiremock": "3.13.9",
    "should": "13.2.3",
    "sinon": "9.2.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "typescript": "4.6.2",
    "webpack": "4.44.2"
  },
  "dependencies": {
    "@signageos/file": "0.1.1",
    "@signageos/sdk": "1.19.0",
    "chalk": "2.4.2",
    "child-process-promise": "2.2.1",
    "cli-progress": "3.8.2",
    "command-line-args": "5.1.1",
    "command-line-usage": "6.0.2",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "globby": "11.1.0",
    "internal-ip": "5.0.0",
    "mime": "2.4.4",
    "node-fetch": "2.6.7",
    "prompts": "2.1.0",
    "semver": "7.1.3",
    "serve-static": "1.14.1"
  }
}
