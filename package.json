{
  "name": "@signageos/cli",
  "version": "0.6.2",
  "main": "./dist/index.js",
  "files": [
    ".env",
    "dist",
    "config",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "repository": {
    "url": "git@gitlab.com:signageos/cli",
    "type": "git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint --config node_modules/@signageos/codestyle/tslint.json {src,tests}/**/*.{ts,tsx}",
    "prebuild": "rm -rf dist/*",
    "prepare": "npm run prebuild && npm run build && chmod +x dist/index.js",
    "test": "env NODE_ENV=test mocha --opts mocha.opts",
    "watch": "tsc --watch",
    "check": "depcheck --specials=tslint,webpack,mocha --parsers='*.ts:typescript,*.js:es6' --detectors='requireCallExpression,importDeclaration' --ignore-dirs='dist' --ignores='@types/*,@signageos/codestyle,mocha,depcheck'"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.3.0"
  },
  "bin": {
    "sos": "./dist/index.js"
  },
  "publishConfig": {
    "registry": "https://npm.signageos.io",
    "access": "public"
  },
  "devDependencies": {
    "@signageos/codestyle": "0.0.12",
    "@types/chalk": "2.2.0",
    "@types/cli-progress": "1.8.1",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/cors": "2.8.6",
    "@types/debug": "4.1.4",
    "@types/express": "4.17.1",
    "@types/file-type": "10.9.1",
    "@types/fs-extra": "8.0.0",
    "@types/globby": "9.1.0",
    "@types/ini": "1.3.30",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.9",
    "@types/node-fetch": "2.5.0",
    "@types/prompts": "2.0.1",
    "@types/read-chunk": "3.1.0",
    "@types/should": "13.0.0",
    "@types/sinon": "7.5.0",
    "@types/webpack": "4.39.1",
    "depcheck": "0.8.3",
    "mocha": "6.2.0",
    "rewiremock": "3.13.9",
    "should": "13.2.3",
    "sinon": "7.5.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@signageos/sdk": "1.2.0",
    "chalk": "2.4.2",
    "cli-progress": "3.3.1",
    "command-line-args": "5.1.1",
    "command-line-usage": "6.0.2",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "file-type": "12.4.0",
    "fs-extra": "8.1.0",
    "globby": "10.0.1",
    "ini": "1.3.5",
    "mime": "2.4.4",
    "node-fetch": "2.6.0",
    "parse-gitignore": "1.0.1",
    "prompts": "2.1.0",
    "read-chunk": "3.2.0",
    "serve-static": "1.14.1",
    "webpack": "4.39.3",
    "webpack-dev-server": "3.8.0"
  }
}
