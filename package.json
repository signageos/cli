{
  "name": "@signageos/cli",
  "version": "2.3.1",
  "main": "./dist/index.js",
  "files": [
    ".env",
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "url": "https://github.com/signageos/cli",
    "type": "git"
  },
  "scripts": {
    "build": "npx check-engine@latest && tsc && npm run build:templates",
    "build:templates": "node tools/include-templates.js",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.json ./src/",
    "lint": "npm run lint:eslint",
    "clean": "rm -rf dist/*",
    "clean-build": "npm run clean && npm run build && chmod +x dist/index.js",
    "test": "env NODE_ENV=test mocha",
    "test:windows": "cross-env NODE_ENV=test mocha",
    "test:coverage": "c8 npm run test",
    "watch": "tsc --watch",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib check-deps && npm run check-circular-deps",
    "check-circular-deps": "npx madge@5 --circular --extensions ts ./src/",
    "depcheck": "depcheck --config .depcheckrc.json",
    "typecheck": "tsc --noEmit",
    "prettier:format": "prettier \"(src|tests|tools)/**/*.+(ts|js)\" --write"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "bin": {
    "sos": "./dist/index.js"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@signageos/codestyle": "1.0.0",
    "@types/archiver": "6.0.3",
    "@types/child-process-promise": "2.2.1",
    "@types/cli-progress": "3.7.0",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.2",
    "@types/cors": "2.8.6",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.1",
    "@types/fs-extra": "8.0.0",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.9",
    "@types/node-fetch": "2.5.0",
    "@types/prompts": "2.0.1",
    "@types/semver": "7.1.0",
    "@types/sinon": "7.5.0",
    "@types/unzipper": "0.10.10",
    "@types/which": "3.0.4",
    "c8": "7.13.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.3",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-unused-imports": "4.1.4",
    "mocha": "8.4.0",
    "rewiremock": "3.13.9",
    "should": "13.2.3",
    "sinon": "20.0.0",
    "ts-node": "8.3.0",
    "typescript": "4.8.4",
    "typescript-eslint": "8.30.1",
    "unzipper": "0.12.3"
  },
  "dependencies": {
    "@signageos/file": "0.1.1",
    "@signageos/sdk": "1.27.4",
    "archiver": "7.0.1",
    "chalk": "2.4.2",
    "child-process-promise": "2.2.1",
    "cli-progress": "3.8.2",
    "command-line-args": "5.1.1",
    "command-line-usage": "6.0.2",
    "cors": "2.8.5",
    "debug": "4.3.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "internal-ip": "5.0.0",
    "mime": "2.4.4",
    "node-fetch": "2.6.7",
    "prompts": "2.1.0",
    "semver": "7.1.3",
    "serve-static": "1.14.1",
    "which": "5.0.0",
    "zod": "3.24.2"
  }
}
